{
  "name": "awesome-android-root",
  "version": "1.1.0",
  "description": "A curated list of best Android root applications and best magisk modules. Features over 200+ apps across categories like system mods, privacy, customization, and development tools.",
  "private": true,
  "homepage": "https://awesome-android-root.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/awesome-android-root/awesome-android-root.git"
  },
  "bugs": {
    "url": "https://github.com/awesome-android-root/awesome-android-root/issues"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "scripts/build-docs.sh && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:build:pwa": "cross-env NODE_ENV=production vitepress build docs",
    "docs:analyze": "cross-env ANALYZE=true vitepress build docs",
    "clean": "rimraf docs/.vitepress/dist docs/.vitepress/cache",
    "clean:pwa": "rimraf docs/.vitepress/dist/sw.js docs/.vitepress/dist/workbox-*.js"
  },
  "devDependencies": {
