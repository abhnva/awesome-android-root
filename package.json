  },
  "optionalDependencies": {
    "workbox-window": "^7.3.0"
  },
  "keywords": [
    "android",
    "root",
    "magisk",
    "rooting",
    "android-apps",
    "awesome-list",
    "lsposed",
    "xposed",
    "kernelsu",
    "android-tools",
    "pwa",
    "vitepress"
